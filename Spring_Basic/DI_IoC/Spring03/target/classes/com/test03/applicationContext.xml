<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="park" class="com.test03.Engineer">
		<constructor-arg name="name" value="박진희"></constructor-arg>
		<constructor-arg name="salary" value="500"></constructor-arg>
		<property name="dept" value="기술팀"></property>
	</bean>

	<bean id="la" class="com.test03.Developer">
		<constructor-arg name="name" value="라현준"></constructor-arg>
		<constructor-arg name="salary" value="500"></constructor-arg>
		<property name="dept" value="기술팀"></property>
	</bean>

</beans>
